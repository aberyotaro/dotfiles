filetype plugin indent on
syntax on
highlight LineNr ctermfg=darkyellow
inoremap <silent> jj <ESC>
inoremap <C-b> <ESC>^i
inoremap <C-e> <End>
inoremap <C-h> <Left>
inoremap <C-l> <Right>
inoremap <C-j> <Down>
inoremap <C-k> <Up>

set title
set cursorline
set showmatch
set number
set clipboard=unnamed
set backspace=indent,eol,start
set hlsearch
set expandtab
set tabstop=4
set shiftwidth=4
set smarttab
set autoindent
set smartindent
set incsearch
set background=dark
set laststatus=2
set ruler
