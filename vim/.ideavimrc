set surround
set clipboard=unnamed
set multiple-cursors
highlight LineNr ctermfg=darkyellow

inoremap <silent> jj <ESC>
inoremap <silent> jk <ESC>
inoremap <C-b> <Esc>^i
inoremap <C-e> <End>
inoremap <C-h> <Left>
inoremap <C-l> <Right>
inoremap <C-j> <Down>
inoremap <C-k> <Up>

set showmatch
set hlsearch
set incsearch
